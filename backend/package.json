{
  "name": "vaisu-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "AWS_REGION=us-east-1 tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/backend/src/server.js",
    "test": "vitest",
    "setup:db": "tsx scripts/setup-dynamodb.ts",
    "setup:s3": "aws s3 mb s3://vaisu-documents-dev --region us-east-1",
    "setup:all": "npm run setup:db && npm run setup:s3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.946.0",
    "@aws-sdk/client-s3": "^3.946.0",
    "@aws-sdk/lib-dynamodb": "^3.946.0",
    "@aws-sdk/s3-request-presigner": "^3.946.0",
    "@types/argon2": "^0.15.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "argon2": "^0.44.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.3",
    "mammoth": "^1.7.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "react-virtualized": "^9.22.6",
    "redis": "^4.6.13",
    "resend": "^6.7.0",
    "stripe": "^20.2.0",
    "uuid": "^13.0.0",
    "vite": "^7.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.17",
    "@types/pdf-parse": "^1.1.4",
    "@types/supertest": "^6.0.3",
    "aws-sdk-client-mock": "^4.1.0",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-arm64-gnu": "^4.55.1"
  }
